int main()
{
    float metros_quadrados, acres;

    printf("Digite uma area em metros quadrados: ");
    scanf("%f", &metros_quadrados);

    acres=metros_quadrados*0.000247;

    printf("O valor em acres e: %f", acres);

    return 0;
}

